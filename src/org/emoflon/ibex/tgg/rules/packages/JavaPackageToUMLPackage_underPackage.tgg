#using MoDiscoIbexTGG.*
#using AttrCondDefLibrary.*

#rule JavaPackageToUMLPackage_underPackage #with MoDiscoIbexTGG

#source { 
	jParent : Package{
		++-ownedPackages->jp
	}
	++ jp : Package
}

#target {
	
	uParent : Package{
		++ -packagedElement->up
	}
	
	++ up : Package
}

#correspondence {
	
	p2p : JavaPackage2UmlPackage{
		#src->jParent
		#trg->uParent
	}
	
	++ jp2up : JavaPackage2UmlPackage{
		#src->jp
		#trg->up
	}
}

#attributeConditions {
	eq_string(up.name,jp.name)
}
